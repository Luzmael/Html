{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/tienda-(.*)",
      "dest": "/tienda-$1/index.html"
    },
    {
      "src": "/generador",
      "dest": "/generador/index.html"
    },
    {
      "src": "/products/(.*)",
      "dest": "/products/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/generador/index.html",
      "status": 200
    }
  ]
}
